
{% extends 'template.html.twig' %}

{% block title %}
Home
{% endblock %}


{% block content %}
<div class="row mx-auto">
    <div class="col-6 offset-3">
        <h1>Participants</h1>


    <form method="post" action="user/addNewUser" class="card">
            <legend class="card-header text-secondary">Upload</legend>
            <div class="form-inline mt-2">
                <label for="file" class="col-5 offset-1 text-right ">
                    Choisir le fichier : </label>
                <input class="form-control col-5" type="file" name="file"
                id="file" required minlength="4">
            </div>
           
            <div class="form-group mt-3">
                <input type="submit" class="btn btn-success"
                value="Uploader" />
            </div>
        </form>





    </div>
</div>
{% endblock %}


